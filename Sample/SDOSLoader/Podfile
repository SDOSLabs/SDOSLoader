source 'http://git.sdos.es/ios/cocoapods-specs.git'
source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
use_modular_headers!
inhibit_all_warnings!

def shared_pods
	pod 'SDOS/Loader', '~> 1.0'
	pod 'SDOSLoader', :path => '../../', :inhibit_warnings => false

    pod 'SDOSLocalizableString'
	pod 'IQKeyboardManager'
end

target :SDOSLoader do
	shared_pods
end
