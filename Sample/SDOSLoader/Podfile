source 'http://git.sdos.es/ios/cocoapods-specs.git'
source 'https://github.com/CocoaPods/Specs.git'

platform :ios, '8.0'
use_frameworks!
inhibit_all_warnings!

def shared_pods
	pod 'SDOSLoader', :path => '../../', :inhibit_warnings => false
  pod 'SDOSCustomLoader', :path => '../../../SDOSCustomLoader', :inhibit_warnings => false
	pod 'IQKeyboardManager'
    
end

target :SDOSLoaderSample do
	shared_pods
end
